    "### Time efficiend C++\n",
    "#include <iostream>\n",
    "using namespace std;\n",
    "\n",
    "int main()\n",
    "{\n",
    "    int inp;\n",
    "    int Num;\n",
    "    cin.tie(NULL);\n",
    "    cout.sync_with_stdio(false);\n",
    "    cin >> inp;\n",
    "    for(int i = 0; i < inp; i++)\n",
    "    {\n",
    "        cin >> Num;\n",
    "\n",
    "        if(Num % 4 == 0)\n",
    "        {\n",
    "            cout << 1 << '\\n';\n",
    "        }\n",
    "        else if(Num % 4 == 1)\n",
    "        {\n",
    "            cout << 7 << '\\n';\n",
    "        }\n",
    "        else if(Num % 4 == 2)\n",
    "        {\n",
    "            cout << 9 << '\\n';\n",
    "        }\n",
    "        else\n",
    "        {\n",
    "            cout << 3 << '\\n';\n",
    "        }\n",
    "    }\n",
    "    return 0;\n",
    "}\n"
   ]
  },
